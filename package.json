{
    "name": "ppx_optcomp-actual",
    "exportedEnvVars": {
        "ppx_optcomp_version": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "113.33.500403"
        },
        "ppx_optcomp_installed": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "true"
        },
        "FINDLIB": {
            "global": true,
            "resolveAsRelativePath": true,
            "globalCollisionBehavior": "joinPath",
            "val": "./_build/ocamlfind/lib"
        },
        "version": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "113.33.500403"
        },
        "ppx_optcomp_enable": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "enable"
        },
        "PATH": {
            "global": true,
            "resolveAsRelativePath": true,
            "globalCollisionBehavior": "joinPath",
            "val": "./_build/ocamlfind/bin"
        }
    },
    "version": "113.33.500403",
    "dependencies": {
        "@opam-alpha/ocamlfind": ">= 1.3.52",
        "dependency-env": "https://github.com/npm-ml/dependency-env.git",
        "substs": "https://github.com/yunxing/substs.git",
        "@opam-alpha/ocamlbuild": "*",
        "@opam-alpha/ppx_core": ">= 113.33.500 < 113.34.500",
        "@opam-alpha/ppx_tools": ">= 0.99.53",
        "nopam": "https://github.com/yunxing/nopam.git",
        "opam-installer-bin": "https://github.com/yunxing/opam-installer-bin.git"
    },
    "scripts": {
        "postinstall": "eval $(dependencyEnv) && nopam && ./configure --prefix $opam_prefix && make && (opam-installer --prefix=$opam_prefix || true)"
    },
    "peerDependencies": {
        "@opam-alpha/ocaml": ">= 4.03.50 < 4.04.50"
    }
}
